<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#BA1414"
    tools:context=".GemeniActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="413dp"
        android:layout_height="73dp"
        android:text="Gemeni"
        android:textAlignment="center"
        android:textColor="#0066ff"
        android:textSize="60dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="-1dp"
        tools:layout_editor_absoluteY="3dp"
        tools:ignore="MissingConstraints"
        android:gravity="center_horizontal" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="427dp"
        android:layout_height="36dp"
        android:text="21May-21Jun"
        android:textSize="30dp"
        android:textAlignment="center"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="-6dp"
        tools:layout_editor_absoluteY="88dp"
        tools:ignore="MissingConstraints"
        android:gravity="center_horizontal" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="394dp"
        android:layout_height="398dp"
        android:text="Dynamic, intense, and with so many shifting parts to their personalities, Gemini love to date. Indeed, they consider dating a skill. Gemini are great at drawing first dates out of their shells, and they rarely have a bad date because this optimism-infused sign will always finds something positive about the person they meet or the conversation they shared."
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="25dp"
        tools:layout_editor_absoluteY="196dp"
        tools:ignore="MissingConstraints"
        android:gravity="center_horizontal" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="406dp"
        android:layout_height="111dp"
        android:text="Most comaptible with Gemeni: Aries, Leo, Libra"
        android:textSize="25dp"
        android:textColor="#0066ff"
        android:textStyle="bold"
        android:textAlignment="center"
        tools:layout_editor_absoluteY="622dp"
        tools:ignore="MissingConstraints"
        android:gravity="center_horizontal" />
</androidx.constraintlayout.widget.ConstraintLayout>